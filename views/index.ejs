<!DOCTYPE html>
<html>
<head>
<title>Coffee Tracker Bot</title>
</head>

<body>
</body>

<script>

function GET(url, callback) {
  fetch(url)
  .then((resp) => resp.json())
  .then(function(data) {
    callback(data)
  })
}

function POST(url, data, callback) {

  fetch(url, {
    method: "POST",
    body: data,
    headers: {
      "Content-Type": "application/x-www-form-urlencoded"
    }
  })
  .then(function(res){ return res.json(); })
  .then(function(data){ callback(data) })

}

</script>

</html>
